{
  "name": "jmanc",
  "version": "1.0.0",
  "description": "Socket Server",
  "main": "index.js",
  "author": "Jesse",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/bin/www.js",
    "build": "rimraf dist && cross-env NODE_ENV=production babel src -d dist -s && cpy '***/*.html' '../dist/' --cwd=src --parents",
    "dev": "nodemon --exec babel-node src/bin/www.js",
    "test": "cross-env NODE_ENV=test jest",
    "cover": "npm run test -- --coverage",
    "lint": "eslint --fix src && prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bristol": "^0.4.0",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "mysql": "^2.15.0",
    "mysql-events": "^0.0.11",
    "palin": "^2.2.0",
    "socket.io": "^2.1.1",
    "yenv": "^2.1.0",
    "zongji": "^0.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2"
  }
}
